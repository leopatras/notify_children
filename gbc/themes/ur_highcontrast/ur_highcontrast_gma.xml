<?xml version="1.0" encoding="UTF-8"?>
  <resources>
    <color name="ur_highcontrast_primary">#ff0c4399</color>
    <color name="ur_highcontrast_primary_text">#ff212121</color>
    <color name="ur_highcontrast_primary_dark">#ff0c4399</color>
    <color name="ur_highcontrast_accent">#ff0c4399</color>
    <color name="ur_highcontrast_activated_background">#ffffffff</color>
    <color name="ur_highcontrast_window_background">#fff5f5f5</color>
    <color name="ur_highcontrast_navigation_bar_background">#ff0c4399</color>
    <color name="ur_highcontrast_action_bar_text">#ffffffff</color>
    
    <style name="ur_highcontrast_style" parent="GeneroMobile.Theme.Client.Browser">
        <item name="colorPrimary">@color/ur_highcontrast_primary</item>
        <item name="colorPrimaryDark">@color/ur_highcontrast_primary_dark</item>
        <item name="actionModeBackground">@color/ur_highcontrast_primary</item>
        <item name="colorAccent">@color/ur_highcontrast_accent</item>
        <item name="colorControlHighlight">@color/ur_highcontrast_accent</item>
        <item name="colorControlActivated">@color/ur_highcontrast_accent</item>
        <item name="android:activatedBackgroundIndicator">@color/ur_highcontrast_activated_background</item>
        <item name="android:windowBackground">@color/ur_highcontrast_window_background</item>
        <item name="android:navigationBarColor">@color/ur_highcontrast_navigation_bar_background</item>
        <item name="android:textColorPrimary">@color/ur_highcontrast_primary_text</item>
        <item name="actionMenuTextColor">@color/ur_highcontrast_action_bar_text</item>
        <item name="android:actionMenuTextColor">@color/ur_highcontrast_action_bar_text</item>
        <item name="actionBarTheme">@style/ur_highcontrast_style.Theme.ActionBar</item>
    </style>
    <style name="ur_highcontrast_style.Theme.ActionBar" parent="GeneroMobile.Theme.ActionBar">
      <item name="android:textColorPrimary">@color/ur_highcontrast_action_bar_text</item>
      <item name="colorControlNormal">@color/ur_highcontrast_action_bar_text</item>
    </style>
  </resources>
